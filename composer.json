{
    "name": "pharaonic/laravel-seo",
    "description": "SEO generates meta tags with quick and easy functions & blade directives.",
    "keywords": [
        "laravel seo",
        "php seo",
        "meta tags",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggi@raggitech.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\SEO\\SEOServiceProvider"
            ],
            "aliases": {
                "SEO": "Pharaonic\\Laravel\\SEO\\SEO"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\SEO\\": "src"
        },
        "files": ["src/helper.php"]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}